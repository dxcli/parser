      ],
      "lint": [
        "eslint .",
        "tsc -p test --noEmit",
        "tslint -p test",
        "commitlint --from origin/master"
      ]
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "dxcli/parser",
  "scripts": {
    "commitmsg": "dxcli-dev-commitmsg",
    "lint": "dxcli-dev lint",
    "precommit": "dxcli-dev lint",
    "prepare": "rm -rf lib && tsc",
    "test": "dxcli-dev test"
  },
  "types": "lib/index.d.ts"
