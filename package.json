{
  "name": "@dxcli/parser",
  "description": "arg and flag parser for dxcli",
  "version": "0.0.2",
  "author": "Jeff Dickey @jdxcode",
  "dependencies": {
    "@dxcli/screen": "^0.0.0",
    "chalk": "^2.3.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@dxcli/dev": "^1.1.3",
    "@dxcli/dev-semantic-release": "^0.0.3",
    "@dxcli/dev-test": "^0.1.1",
    "@dxcli/dev-tslint": "^0.0.9",
    "@heroku/linewrap": "^1.0.0",
    "@types/chalk": "^2.2.0",
    "@types/node": "^9.3.0",
    "eslint": "^4.15.0",
    "eslint-config-dxcli": "^1.1.4",
    "husky": "^0.14.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "dxcli": {
    "workflows": {
      "test": [
        "eslint .",
